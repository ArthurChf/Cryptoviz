{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/jobs/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/jobs/tsconfig.app.json"
  },
  "projects": {
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "scrapper": {
      "type": "application",
      "root": "apps/scrapper",
      "entryFile": "main",
      "sourceRoot": "apps/scrapper/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scrapper/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "data": {
      "type": "application",
      "root": "apps/data",
      "entryFile": "main",
      "sourceRoot": "apps/data/src",
      "compilerOptions": {
        "tsConfigPath": "apps/data/tsconfig.app.json"
      }
    },
    "kakfa": {
      "type": "library",
      "root": "libs/kakfa",
      "entryFile": "index",
      "sourceRoot": "libs/kakfa/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kakfa/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/jobs"
}